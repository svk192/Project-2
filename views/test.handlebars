    {{#each books}}
    <div class="card">
        <div class="row no-gutters">
            <div class="col-auto">
                <img src="{{this.volumeInfo.imageLinks.thumbnail}}" class="img-fluid" alt=""
                    style="width:250px;height:300px">
            </div>
            <div class="col">
                <div class="card-block px-2">
                    <h5 class="bookTitle">{{this.volumeInfo.title}}</h5>
                    <h6 class="bookAuthor">{{this.volumeInfo.authors}}</h6>
                    <p class="bookPlot">{{this.volumeInfo.description}}</p>
                    <button class="BookButton btn btn-primary" id="bookAdded" data-idname="{{this.id}}" id="{{this.id}}"
                        data-title="{{this.volumeInfo.title}}"> Add Book!</a>
                </div>
                <div class="d-flex justify-content-end">
                    <p>{{this.votenumber}}</p>
                    <button type="button" class="btn btn-secondary upvote"  data-bookid="{{this.id}}">&#128077;</button>
                    <button type="button" class="btn btn-secondary downvote">&#128078;</button>
                </div>
            </div>
           
        </div>
    </div>
     {{/each}}