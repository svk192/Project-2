                    <p class="bookPlot">{{this.volumeInfo.description}}</p>
                     <button class="BookButton btn btn-primary" data-idname="{{this.id}}" id="{{this.id}}" data-title="{{this.volumeInfo.title}}"> Add Book!</a>
                </div>
