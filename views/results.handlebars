
<div class="row">
   <ul>
     {{#each books}}
     <li>
       <h2>{{this.Book.title}}</h2>
       <h3>Votes: {{this.Count}}</h3>
       <h3>author: {{this.Book.author}}</h3>
       <p>description: {{this.Book.description}}</p>
       <p>isbn_type: {{this.Book.ISBN_type}}</p>
       <p>isbn_id: {{this.Book.ISBN_ID}}</p>
       <p>page count: {{this.Book.pageCount}}</p>
       <p>category: {{this.Book.category}}</p>
     </li>
     {{/each}}
   </ul>
 </div>
 <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="js/addbook.js"></script>


